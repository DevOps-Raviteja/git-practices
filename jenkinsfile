pipeline {
    agent any 
    tools {
        jdk 'jdk-17'
    }
    stages {
        stage ("Git Checkout"){
            steps {
                git branch: 'main', url 'https://github.com/DevOps-Raviteja/git-practices.git'
            }
        }
        stage ('Code Compile'){
            steps {
                sh 'mvn compile'
            }
        }
        stage ('Code Compile'){
            steps {
                sh 'mvn compile'
            }
        }
    }
}